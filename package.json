    "react-native-image-resizer": "^1.0.0",
    "react-native-in-app-utils": "^6.0.0",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-lightbox": "^0.7.0",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-maps": "^0.19.0",
    "react-native-masked-text": "^1.6.4",
    "react-native-message-bar": "2.0.9",
    "react-native-modal": "https://github.com/react-native-community/react-native-modal.git#f085f39ebb4cff7a73968dc4d47828374f5b3d94",
    "react-native-nav": "^2.0.2",
    "react-native-notifications": "^1.2.0",
