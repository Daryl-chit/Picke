    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.19.0",
    "husky": "^0.13.1",
    "ignite-animatable": "^1.0.0",
    "ignite-ir-boilerplate": "^2.2.2",
